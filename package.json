{
  "name": "rest-api-tutorial",
  "version": "1.0.0",
  "engines": {
    "node": "17.9.0"
  },
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "lint": "eslint .",
    "test": "cross-env NODE_ENV=test jest --testTimeout=10000 --watchAll"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "aws-sdk": "^2.1158.0",
    "cors": "^2.8.5",
